<h2>View Paddock:</h2>

<form action="/paddocks/$paddock.getId()/edit" method="post">

    <label for="name">Name</label>
    <input id="name" name="name" value="$paddock.getName()" type="text">

    <label for="capacity">Capacity</label>
    <input id="capacity" name="capacity" value="$paddock.getCapacity()" type="number">

    <input class="button" type="submit" value="Save">

</form>